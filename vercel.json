{
    "version": 2,
    "builds": [
      {
        "src": "back/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "front",
        "command": "npm run build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/back/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "front/dist"
      }
    ]
  }