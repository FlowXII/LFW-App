{
  "version": 2,
  "builds": [
    {
      "src": "back/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "front/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}