{
  "version": 2,
  "builds": [
    {
      "src": "back/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "front/dist/**/*",
      "use": "@vercel/static",
      "config": {
        "distDir": "front/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "back/server.js"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/front/dist/index.html"
    }
  ]
}